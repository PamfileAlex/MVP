<Page x:Class="Tema3_School_Platform.Views.ProfessorPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Tema3_School_Platform.Views"
      xmlns:vm="clr-namespace:Tema3_School_Platform.ViewModels"
      mc:Ignorable="d" 
      d:DesignHeight="860" d:DesignWidth="1440"
      Background="{StaticResource Background}"
      Title="ProfessorPage">

    <Page.DataContext>
        <vm:ProfessorPageVM/>
    </Page.DataContext>

    <Grid>
        <TextBlock Text="PROFESSOR" FontSize="100" Foreground="{StaticResource Text}" HorizontalAlignment="Center" VerticalAlignment="Center"/>
    </Grid>
</Page>
